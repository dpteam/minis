<Files .htaccess>
    order allow,deny
    deny from all
</Files>

Options All -Indexes

RewriteEngine on
RewriteBase /simpson/
RewriteCond %{REQUEST_FILENAME} !-l
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^([a-zA-ZäöüÄÖÜß0-9_-]*)(?:/([a-zA-ZäöüÄÖÜß0-9_-]*))?(?:/([a-zA-ZäöüÄÖÜß0-9_-]*))?$ index.php?p=$1&s=$2&t=$3 [L,QSA]